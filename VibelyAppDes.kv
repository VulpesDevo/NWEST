                            size:"200dp","45dp"
                            pos_hint:{"center_x":.5, "center_y":.2}
                            MDLabel:
                                text:"VIBELY"
                                font_name:"../new/Audiowide/Audiowide-Regular.ttf"
                                font_size:"40sp"
                                bold: True
                                md_bg_color:0,0,0,1
                                color:184/255, 98/255, 27/255,1
                                halign:"center"
                MDCard:
                    md_bg_color:'#A85A19'
