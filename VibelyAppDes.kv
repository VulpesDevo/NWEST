<Search_Result@OneLineAvatarListItem>:
    id: search_list

    text_list: ''

    divider: 'Full'

    Label:
        text: root.text_list
        size_hint_y: None
        font_size: '15sp'
        height: self.texture_size[1]
        text_size: (self.width - dp(100), None)
        pos_hint: {"center_y": 0.5, "x": (dp(90)) / self.parent.width }

    ImageLeftWidget:

        FitImage:
            id: search_pic
            source: 'mobilelegends.jpg'
            size_hint: None, None
            pos_hint: {'center_y': 0.5}

<Content@MDCard>:
    size_hint: None, None
    size: "100dp", "150dp"

    FitImage:
        id: image_widget

<SuccessRegistration>:
    auto_dismiss: False
    md_bg_color: 0,0,0,0
    MDCard:
        md_bg_color:"#190F28"
        size_hint: None, None
        size: "275dp", "350dp"
        radius:[20,]
        MDFloatLayout:
            padding: "10dp"
            MDCard:
                md_bg_color: 0,0,0,0
                size_hint: None, None
                size: "150dp", "150dp"
                pos_hint: {"center_x": .5, "center_y": .75}
                MDFloatLayout:
                    MDIconButton:
                        icon: "../new/images/regis.png"
                        disabled: True
                        icon_size: "200sp"
                        pos_hint: {"center_x": .5, "center_y": .5}
            Label:
                md_bg_color:0,0,0,0
                pos_hint: {"center_x": .5, "center_y": .5}
                text: "Congratulations!\nYou're now officially Vibely user."
                halign: "center"
            MDFillRoundFlatButton:
                text: "OK"
                bold:True
                font_size:"20sp"
                pos_hint: {"center_x": .5, "center_y": .2}
                size_hint: .5,.1
                on_release:root.dismiss()
